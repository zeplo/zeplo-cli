{
  "name": "@zeplo/cli",
  "version": "0.2.0",
  "description": "CLI for Zeplo",
  "preferGlobal": true,
  "repository": "https://github.com/zeplo/zeplo-cli",
  "author": "Calum Moore",
  "license": "MIT",
  "files": [
    "dist",
    "install.js"
  ],
  "scripts": {
    "build": "babel ./src --ignore .spec.js -d dist",
    "prepare": "yarn run build && cp default.js dist/zeplo",
    "postinstall": "node ./install.js",
    "fix": "yarn eslint \"./src/**/*.{js,jsx}\" --fix"
  },
  "bin": {
    "zeplo": "dist/zeplo"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "async-retry": "^1.3.3",
    "chalk": "^2.4.2",
    "death": "^1.1.0",
    "node-fetch": "^3.2.1",
    "progress": "^2.0.3",
    "read-pkg": "^7.1.0",
    "which-promise": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest-node": "^5.5.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "flow-bin": "^0.173.0",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "pkg": "5.5.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "gitHead": "d825a519a2afdbd4fda32a2a0afd84ab8606b6cd"
}
